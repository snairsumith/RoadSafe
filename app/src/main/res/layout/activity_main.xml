<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@color/bgColor">
<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="201dp"
    android:id="@+id/linearLayout">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:id="@+id/imageView"
        android:src="@drawable/blockicon"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="0dp"
        />
</LinearLayout>
<LinearLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/lir2"
    android:orientation="vertical"
    android:layout_marginTop="200dp"
    >
    <TextView
        android:layout_width="197dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="95dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="   Drive Mode"
        android:id="@+id/textView"
        android:textSize="25dp"
        android:textColor="#fffffbf9"
        android:shadowColor="#ffffdedb"
        android:layout_below="@+id/lir2"
        android:layout_centerHorizontal="true" />

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/toggleButton"
        android:checked="false"
        android:background="@color/buttonBgcolor"
        android:width="20dp"
        android:height="10dp"
        android:textOff="Yes"
        android:textOn="No"
        android:textSize="20dp"
        android:textColor="@color/bgColor"
        android:textStyle="bold"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="150dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="   Panic Mode"
        android:id="@+id/textView2"
        android:textSize="25dp"
        android:textColor="#fffffbf9"
        android:shadowColor="#ffffdedb"
        android:layout_below="@+id/toggleButton"
        android:layout_alignEnd="@+id/textView"
        android:layout_marginTop="15dp"
        android:layout_gravity="center_horizontal" />

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/toggleButton2"
        android:checked="false"
        android:background="@color/buttonBgcolor"
        android:width="20dp"
        android:height="10dp"
        android:textOff="Yes"
        android:textSize="20dp"
        android:textColor="@color/bgColor"
        android:textStyle="bold"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/toggleButton"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="150dp" />

